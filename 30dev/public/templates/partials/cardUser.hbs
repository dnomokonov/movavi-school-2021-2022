<div class="user__box">
    {{#getUser}}
        {{#each data}}
        <div class="person">
            <h1>{{username}}</h1>
            {{#if avatar}}
            <img src="{{avatar}}" alt="">
            {{else}}
            <img src="" alt="">
            {{/if}}
            <div class="text">
            <h3>Level: {{level}}</h3>
            <h3>Money: {{money}}</h3>
            <h3><a href="/user/{{username}}/inventory">Inventory: {{inventory}}</a></h3>
            </div>
        </div>
        {{/each}}
    {{/getUser}}
</div>